{
  "name": "dhis-web-custom-report",
  "private": true,
  "version": "1.0.0",
  "description": "A tutorial application for AngularJS",
  "repository": "https://github.com/angular/angular-phonecat",
  "license": "MIT",
  "dependencies": {
    "angular": "1.3.15",
    "angular-animate": "1.3.15",
    "angular-cookies": "1.3.15",
    "angular-messages": "1.3.15",
    "angular-resource": "1.3.15",
    "angular-route": "1.3.15",
    "angular-sanitize": "1.3.15",
    "angular-translate":"2.7.0",
    "angular-ui-bootstrap":"0.14.3",
    "bootstrap": "3.3.6",
    "jquery": "3.3.x"
  },
  "devDependencies": {
    "angular-mocks": "1.3.15",
    "cpx": "^1.5.0",
    "http-server": "^0.11.1",
    "jasmine-core": "^3.5.0",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.3.0",
    "karma-jasmine": "^1.1.2",
    "protractor": "^5.4.4"
  },
  "scripts": {
    "build": "cd app && zip -r dhis-web-custom-report.zip . &&  cp dhis-web-custom-report.zip /mnt/hgfs/development/dha/dhis-web-custom-report && rm dhis-web-custom-report.zip",
    "postinstall": "",
    "update-deps": "npm update",
    "postupdate-deps": "",
    "copy-bootstrap": "cpx \"node_modules/{bootstrap/dist/*\" app/vendor/bootstrap -C",
    "copy-libs": "cpx \"node_modules/{angular,angular-*,angular-ui-bootstrap\" app/vendor/javascripts -C",
    "copy-jlibs": "cpx \"node_modules/{bootstrap/dist,jquery/dist}/**/*\" app/vendor/javascripts/jquery -C",
    "prestart": "yarn install",
    "start": "http-server ./app -a localhost -p 3000 -c-1",
    "pretest": "yarn install",
    "test": "karma start karma.conf.js",
    "test-single-run": "npm test -- --single-run",
    "preupdate-webdriver": "yarn install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor e2e-tests/protractor.conf.js"
  }
}
